{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "ContrConf",
  "description": "contract configuration, spanning all `token_ids` generated by this contract",
  "type": "object",
  "required": [
    "contract_address",
    "minters",
    "prng_seed",
    "token_id_list",
    "tx_cnt"
  ],
  "properties": {
    "admin": {
      "anyOf": [
        {
          "$ref": "#/definitions/HumanAddr"
        },
        {
          "type": "null"
        }
      ]
    },
    "contract_address": {
      "$ref": "#/definitions/HumanAddr"
    },
    "minters": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/HumanAddr"
      }
    },
    "prng_seed": {
      "type": "array",
      "items": {
        "type": "integer",
        "format": "uint8",
        "minimum": 0.0
      }
    },
    "token_id_list": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "tx_cnt": {
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    }
  },
  "definitions": {
    "HumanAddr": {
      "type": "string"
    }
  }
}
